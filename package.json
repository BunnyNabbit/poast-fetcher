{
  "name": "poast-fetcher",
  "displayName": "Poast Fetcher",
  "publisher": "BunnyNabbit",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.104.0"
  },
  "license": "MIT",
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "poast-fetcher.configure",
        "title": "Poast Fetcher: Configure ATProto account"
      },
      {
        "command": "poast-fetcher.writePost",
        "title": "Poast Fetcher: Write simple text post"
      },
      {
        "command": "poast-fetcher.openFeed",
        "title": "Poast Fetcher: Open a feed"
      }
    ],
    "views": {
      "feed": [
        {
          "id": "feed",
          "name": "Feed",
          "icon": "media/dep.svg"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.104.0",
    "@types/vscode-webview": "^1.57.5",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.36.0",
    "prettier": "^3.6.2",
    "prettier-plugin-drone-class": "^0.1.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@atproto/api": "^0.17.0",
    "@atproto/oauth-client-node": "^0.3.9",
    "hls.js": "0.8.5"
  }
}